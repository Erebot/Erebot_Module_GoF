msgid   "player.wins.normal"
msgstr  "<var name=\"logo\"/> <b><var name=\"nick\"/></b> wins this round! The "
        "next round will start in <var name=\"delay\"/> seconds."
#
msgid   "player.wins.final"
msgstr  "<var name=\"logo\"/> <b><var name=\"nick\"/></b> wins this round!"
#
msgid   "player.upper-hand"
msgstr  "<var name=\"logo\"/> <b><var name=\"nick\"/></b> has got the upper "
        "hand and may now start a new combination"
#
msgid   "player.turn.self"
msgstr  "<var name=\"logo\"/> <b><var name=\"nick\"/></b>: it's your turn "
        "sleepyhead!"
#
msgid   "player.turn.other"
msgstr  "<var name=\"logo\"/>: It's <b><var name=\"nick\"/></b>'s turn."
#
msgid   "player.plays"
msgstr  "<var name=\"logo\"/>: <b><var name=\"nick\"/></b> plays <var name="
        "\"type\"/>: <for from=\"cards\" item=\"card\" separator=\" \"><var "
        "name=\"card\"/></for>"
#
msgid   "player.passes"
msgstr  "<var name=\"logo\"/> <b><var name=\"nick\"/></b> passes turn."
#
msgid   "player.new-combo"
msgstr  "<var name=\"logo\"/> No player dared to raise the heat! It's <b><var "
        "name=\"nick\"/></b>'s turn to play a new combination."
#
#
msgid   "player.last-card"
msgstr  "<var name=\"logo\"/>: <b><var name=\"nick\"/></b> plays <var name="
        "\"type\"/>: <for from=\"cards\" item=\"card\" separator=\" \"><var "
        "name=\"card\"/></for> - This is <b><var name=\"nick\"/></b>'s last "
        "card!"
#
msgid   "player.joins"
msgstr  "<b><var name=\"nick\"/></b> joins this <var name=\"logo\"/> game."
#
msgid   "player.cards"
msgstr  "Your cards: <for from=\"cards\" item=\"card\" separator=\" \"><var "
        "name=\"card\"/></for>"
#
msgid   "player.best-card"
msgstr  "<var name=\"logo\"/>: <b><var name=\"nick\"/></b>, since there is "
        "only 1 card left in <b><var name=\"next_player\"/></b>'s hand, you "
        "MUST play your best card or a combination on this turn!"
#
msgid   "game.winner"
msgstr  "<var name=\"logo\"/> The game was won by <for from=\"winners\" item="
        "\"winner\"><b><var name=\"winner\"/></b></for> with only <b><var name="
        "\"score\"/></b> points after <var name=\"playtime\"/> and <var name="
        "\"rounds\"/> rounds."
#
msgid   "game.starting"
msgstr  "<var name=\"logo\"/>: The game starts now. <b><var name=\"starter\"/"
        "></b>, you must start this game. If you have the <var name=\"m1\"/>, "
        "you MUST play it (alone or in a combination)."
#
msgid   "game.start-delay"
msgstr  "The game will start in <var name=\"delay\"/> seconds."
#
msgid   "game.scores"
msgstr  "<var name=\"logo\"/>: Scores: <for from=\"scores\" key=\"nick\" item="
        "\"score\"><b><var name=\"nick\"/></b>: <var name=\"score\"/></for>."
#
msgid   "game.round.new"
msgstr  "<var name=\"logo\"/> This is round #<b><var name=\"round\"/></b>, "
        "starting after <var name=\"playtime\"/>. <b><var name=\"last_winner\"/"
        "></b>, you must now choose a card to give to <b><var name=\"last_loser"
        "\"/></b>. You will receive <var name=\"card\"/>. Please choose with: "
        "<var name=\"trigger\"/> &lt;card&gt;."
#
msgid   "game.round.first"
msgstr  "<var name=\"logo\"/> This is the first round and you have the <var "
        "name=\"m1\"/> <b><var name=\"nick\"/></b>. You <b>must</b> play it "
        "alone or in a combination!"
#
msgid   "game.order"
msgstr  "<var name=\"logo\"/>: playing turn: <for from=\"nicks\" item=\"nick"
        "\"><b><var name=\"nick\"/></b></for>."
#
msgid   "game.not-started"
msgstr  "The <var name=\"logo\"/> game has not yet started!"
#
msgid   "game.new"
msgstr  "Ok! A new <var name=\"logo\"/> game has been created in <var name="
        "\"chan\"/>. Say \"<var name=\"trigger\"/>\" to join it."
#
msgid   "game.hands"
msgstr  "<var name=\"logo\"/>: Hands: <for from=\"hands\" key=\"nick\" item="
        "\"nb_cards\"><b><var name=\"nick\"/></b>: <var name=\"nb_cards\"/></"
        "for>."
#
msgid   "game.exchange"
msgstr  "<var name=\"logo\"/>: Exchanges: <b><var name=\"winner\"/></b> "
        "receives a <var name=\"received\"/> and gives a <var name=\"given\"/> "
        "to <b><var name=\"loser\"/></b>. <b><var name=\"winner\"/></b>, you "
        "may now start this round."
#
msgid   "game.duration"
msgstr  "This <var name=\"logo\"/> game has been running for <var name="
        "\"playtime\"/>."
#
msgid   "game.discard.empty"
msgstr  "<var name=\"logo\"/>: No card has been played in this round yet."
#
msgid   "game.discard.current"
msgstr  "Current discard: <for from=\"cards\" item=\"card\" separator=\" "
        "\"><var name=\"card\"/></for> (<var name=\"type\"/> played by <b><var "
        "name=\"player\"/></b>)"
#
msgid   "error.trigger.others"
msgstr  "Unable to register triggers for <var name=\"logo\"/> game!"
#
msgid   "error.trigger.create"
msgstr  "Could not register Gang of Four creation trigger"
#
msgid   "error.play"
msgstr  "<var name=\"logo\"/> Now is not the time to play <b><var name=\"nick"
        "\"/></b>!"
#
msgid   "error.pass"
msgstr  "<var name=\"logo\"/> Now is not the time to pass <b><var name=\"nick"
        "\"/></b>!"
#
msgid   "error.not-enough"
msgstr  "<var name=\"logo\"/> Sorry <b><var name=\"nick\"/></b>, but this is "
        "not enough to take the leadership!"
#
msgid   "error.missing-cards"
msgstr  "<var name=\"logo\"/> You do not have the cards required for that move "
        "<b><var name=\"nick\"/></b>!"
#
msgid   "error.missing-card"
msgstr  "<var name=\"logo\"/> Sorry <b><var name=\"nick\"/></b>, you do not "
        "have that card! (<var name=\"card\"/>)"
#
msgid   "error.limit"
msgstr  "Bad limit in the configuration file (<var name=\"limit\"/>). The <var "
        "name=\"logo\"/> game cannot start."
#
msgid   "error.exchange-first"
msgstr  "<var name=\"logo\"/> Proceed with the card exchange first. Use "
        "<b><var name=\"trigger\"/> &lt;card&gt;</b> to select the &lt;"
        "card&gt; to give away."
#
msgid   "error.enough-players"
msgstr  "<var name=\"logo\"/> This game may only be played by 3-4 players and "
        "you cannot join it once it started."
#
msgid   "error.cannot-exchange"
msgstr  "<var name=\"logo\"/> Uh <b><var name=\"nick\"/></b>? No cards need to "
        "be exchanged for now..."
#
msgid   "error.already-running"
msgstr  "A <var name=\"logo\"/> managed by <b><var name=\"admin\"/></b> is "
        "already running. Say \"<var name=\"trigger\"/>\" to join it."
#
msgid   "error.already-in"
msgstr  "<var name=\"logo\"/> You are already part of that game <b><var name="
        "\"nick\"/></b>!"
#
msgid   "combo.unknown"
msgstr  "<var name=\"logo\"/> Hmm? What move was that <b><var name=\"nick\"/></"
        "b>?"
#
