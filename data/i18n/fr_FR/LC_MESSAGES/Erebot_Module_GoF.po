msgid "a pair"
msgstr "une paire"

# File: src/GoF.php, line: 1072
msgid "a single"
msgstr "une carte seule"

# File: src/GoF.php, line: 1081
msgid "a straight"
msgstr "une suite"

# File: src/GoF.php, line: 1090
msgid "a straight flush"
msgstr "une quinte flush"

