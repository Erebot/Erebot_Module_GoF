msgstr  "<var name=\"admin\"/> stopped this <var name=\"logo\"/> game!"
#
msgid   "admin.not-started"
msgstr  "<var name=\"logo\"/> No game has been started in <var name=\"chan\"/> "
        "yet! Nothing to stop."
