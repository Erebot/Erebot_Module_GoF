msgstr  "three of a kind"
#
msgid   "combo.straight-flush"
msgstr  "a straight flush"
#
msgid   "combo.straight"
msgstr  "a straight"
#
msgid   "combo.single"
msgstr  "a single"
#
msgid   "combo.pair"
msgstr  "a pair"
#
msgid   "combo.gang"
msgstr  "a gang"
#
msgid   "combo.full-house"
msgstr  "a full house"
#
msgid   "combo.flush"
msgstr  "a flush"
#
msgid   "card.unknown"
msgstr  "<var name=\"logo\"/> Hmm? What is that card <b><var name=\"nick\"/></"
        "b>?"
#
